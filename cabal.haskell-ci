branches: master ci-* release*

installed: +all -transformers

constraint-set mtl-2.3
  ghc: >=8.6 && < 9 || >=9.2
  constraints: mtl ^>=2.3
  tests: True
  run-tests: True

constraint-set bytestring-0.12
  ghc: >=8.2
  constraints: bytestring ^>=0.12.0.0
  tests: True
  run-tests: True
raw-project
  allow-newer: bytestring
